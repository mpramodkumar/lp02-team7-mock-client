{
  "name": "accelerator",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "custom-react-scripts": "^0.0.23",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.0",
    "husky": "^0.13.3",
    "jest-junit": "^2.1.0",
    "lint-staged": "^3.4.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "1.2.1",
    "sass-lint": "^1.10.2"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "base64-arraybuffer": "^0.1.5",
    "history": "^4.6.2",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "raf": "^3.3.2",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-html-parser": "^1.0.3",
    "react-md": "^1.0.11",
    "react-redux": "^5.0.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "scroll-to-element": "^2.0.0",
    "tween": "^0.9.0",
    "webfontloader": "^1.6.27"
  },
  "scripts": {
    "build": "npm run lint && npm run test:once && react-scripts build",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "lint": "eslint src/**/*.js",
    "sass-lint": "sass-lint src/assets/stylesheets/**/*.scss -v",
    "lint-all": "npm run lint && npm run sass-lint",
    "lint:watch": "esw -w src/**/*.js",
    "test": "react-scripts test --env=jsdom --maxWorkers=2",
    "test:once": "CI=true react-scripts test --env=jsdom --maxWorkers=2 --testResultsProcessor='../../jest-junit'"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write '{app,__{tests,mocks}__}/**/*.js'",
      "git add"
    ]
  },
  "jest": {
    "testResultsProcessor": "./jest/jest-junit"
  }
}
